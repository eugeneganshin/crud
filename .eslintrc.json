{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
      ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "object-curly-newline": "off",
        "no-unused-vars": "off",
        "implicit-arrow-linebreak":"off",
        "no-param-reassign": "off"
    }
}
